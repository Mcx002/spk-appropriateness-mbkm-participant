{
  "name": "mbkm-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "is-ci || husky install",
    "dev": "next dev -p 3009",
    "build": "next build",
    "start": "next start -p 3009",
    "export": "next export",
    "clean": "rimraf --no-glob ./.next ./.next-dev ./out ./.firebase",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-staged-files": "lint-staged --allow-empty",
    "format": "prettier -c src",
    "format:fix": "prettier -w src",
    "typecheck": "tsc --noEmit --incremental false",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@headlessui/react": "^1.7.13",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "clsx": "^1.2.1",
    "cookies-next": "^2.1.1",
    "crypto": "^1.0.1",
    "date-fns": "^2.3.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "next": "13.2.4",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.0.0",
    "qs": "^6.11.1",
    "react": "18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hot-toast": "^2.4.0",
    "react-paginate": "^8.1.5",
    "react-redux": "^8.0.5",
    "tailwind-merge": "^1.10.0",
    "typescript": "5.0.2",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@next/env": "^13.2.4",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.2.0",
    "@types/qs": "^6.9.7",
    "@types/react-date-range": "^1.4.4",
    "@types/react-google-recaptcha": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,mjs}": [
      "yarn format"
    ],
    "src/**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "yarn format"
    ]
  },
  "release": {
    "branches": [],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "breaking",
              "release": "major"
            },
            {
              "type": "build",
              "release": "minor"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "lint",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "Add changelog for ${nextRelease.version}"
        }
      ]
    ]
  },
  "engines": {
    "node": ">=16 <17",
    "yarn": ">=1.22 <2"
  }
}
